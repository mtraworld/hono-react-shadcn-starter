{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext", "DOM"],
		"types": ["vite/client", "@cloudflare/workers-types/2023-07-01"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./app",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
